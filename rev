using System;
 class rev
{
    public static  void Main(string[] args)
    {
        Console.WriteLine("enter a string");
        string str=Console.ReadLine();
        string ans = "";
            for(int i=str.Length -1;i>=0; i-- )
        {
            ans += str[i] + "";
        }
        Console.WriteLine(ans);

    }

}
